<div class="container">
    <div class="card-deck">
        <% for(var j=0; j< tours.length; j++){ %>
        <div class="card border-secondary">
            <img src="<%= tours[j].places[0].image_path %>" class="card-img-top" alt="..." width=300 height=300>
            <div class="card-body">
                <h5 class="card-title"><a href="/tours/<%= tours[j].t_id %>"><%= tours[j].name %></a></h5>
                <p class=" card-text">
                    <h6 class="card-subtitle mb-2">Places Covered:</h6>
                    <ul>
                        <% for(var i=0; i< tours[j].places.length; i++){ %>
                        <li class="card-text">
                            <small class="text-muted"><%= tours[j].places[i].name %></small>
                        </li>
                        <% } %>
                    </ul>
                </p>
            </div>
            <div class="card-footer">
                Region: <span class="badge badge-dark"><%= tours[j].region %></span>
            </div>
        </div>
        <% } %>
    </div>
</div>
