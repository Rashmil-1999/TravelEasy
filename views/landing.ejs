<section id="team" class="pb-5">
    <div class="container">
        <h5 class="section-title h1">Hear Our Story...</h5>
        <div class="row">
            <!-- Review of member 1 -->
            <div class="col-xs-12 col-sm-6 col-md-4">
                <div class="image-flip" ontouchstart="this.classList.toggle('hover');">
                    <div class="mainflip">
                        <div class="frontside">
                            <div class="card">
